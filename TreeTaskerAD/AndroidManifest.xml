<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.vaushell.treetasker"
    android:versionCode="1"
    android:versionName="1.0" >

    <uses-sdk
        android:minSdkVersion="8"
        android:targetSdkVersion="8" />

    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:icon="@drawable/ic_launcher"
        android:label="@string/app_name" >
        <activity android:name=".application.activity.TT_TaskListActivity" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".application.activity.TT_ConnectionActivity" >
        </activity>
        <activity android:name=".application.activity.TT_RegisterActivity" >
        </activity>
        <activity
            android:name="TreeTaskerADactivity"
            android:label="@string/app_name" >
        </activity>
        <activity android:name="TreeTaskerConnectionActivity" >
        </activity>
        <activity android:name=".application.activity.TT_EditTaskActivity" >
        </activity>
    </application>

</manifest>